{
  "module": {
    "name": "feature_daojia",
    "type": "feature",
    "description": "$string:module_desc",
    "mainElement": "Feature_daojiaAbility",
    "deviceTypes": [
      "phone",
      "tablet",
      "2in1"
    ],
    "deliveryWithInstall": true,
    "installationFree": false,
    "pages": "$profile:main_pages",
    "abilities": [
      {
        "name": "Feature_daojiaAbility",
        "srcEntry": "./ets/feature_daojiaability/Feature_daojiaAbility.ets",
        "description": "$string:Feature_daojiaAbility_desc",
        "icon": "$media:layered_image",
        "label": "$string:Feature_daojiaAbility_label",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background",
        "exported": true
      },
      {
        "name": "ability_classify",
        "srcEntry": "./ets/ability_classify/ability_classify.ets",
        "description": "$string:ability_classify_desc",
        "icon": "$media:layered_image",
        "label": "$string:ability_classify_label",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background"
      }
    ],
    "extensionAbilities": [
      {
        "name": "accessibility_classify",
        "srcEntry": "./ets/accessibility_classify/accessibility_classify.ets",
        "description": "$string:accessibility_classify_desc",
        "label": "$string:accessibility_classify_label",
        "type": "accessibility",
        "metadata": [
          {
            "name": "ohos.accessibleability",
            "resource": "$profile:accessibility_classify"
          }
        ]
      }
    ]
  }
}